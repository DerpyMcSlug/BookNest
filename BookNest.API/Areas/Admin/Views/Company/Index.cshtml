@model List<Company>

<div class="container admin-header">
    <h2>Company List</h2>

    <button class="btn btn-primary shadow-soft"
            onclick="openAdminModal('/admin/company/upsert')">
        <i class="bi bi-plus-circle"></i> New Company
    </button>
</div>
<table id="tableCompany" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Street Address</th>
            <th>City</th>
            <th>State</th>
            <th>Postal Code</th>
            <th>Phone Number</th>
            <th></th>
        </tr>
    </thead>
</table>